<script language="javascript">

    //Utils
    Array.prototype.unique = function() {
        var o = {}, i, l = this.length, r = [];
        for(i=0; i<l;i+=1) o[this[i]] = this[i];
        for(i in o) r.push(o[i]);
        return r;
    };

    function sortUnorderedList(ul, sortDescending) {
      if(typeof ul == "string")
        ul = document.getElementById(ul);

      // Get the list items and setup an array for sorting
      var lis = ul.getElementsByTagName("LI");
      var vals = [];

      // Populate the array
      for(var i = 0, l = lis.length; i < l; i++)
        vals.push(lis[i].innerHTML);

      // Sort it
      vals.sort();

      // Sometimes you gotta DESC
      if(sortDescending)
        vals.reverse();

      // Change the list on the page
      for(var i = 0, l = lis.length; i < l; i++)
        lis[i].innerHTML = vals[i];
    }

    var galleryObjects = {"artwork_208307":{"artwork_link":"..\/208307","object_title":"Untitled","object_date_begin":"1\/1\/1954","object_date_end":"12\/31\/1964","object_image_thumbnail":"..\/images\/182118_2982356.jpg","object_image_medium":"..\/images\/182118_2982355.jpg","object_date_display":"c. 1959","object_artist_culture_display":"Roy Lichtenstein<br \/>\nAmerican (1923-1997)","object_artist_sort_name":"Lichtenstein, Roy","object_tag_list":""},"artwork_208310":{"artwork_link":"..\/208310","object_title":"Untitled","object_date_begin":"1\/1\/1955","object_date_end":"12\/31\/1965","object_image_thumbnail":"..\/images\/182160_2983044.jpg","object_image_medium":"..\/images\/182160_2983043.jpg","object_date_display":"c. 1960","object_artist_culture_display":"Roy Lichtenstein<br \/>\nAmerican (1923-1997)","object_artist_sort_name":"Lichtenstein, Roy","object_tag_list":""},"artwork_208306":{"artwork_link":"..\/208306","object_title":"Untitled","object_date_begin":"1\/1\/1959","object_date_end":"12\/31\/1959","object_image_thumbnail":"..\/images\/178285_2963556.jpg","object_image_medium":"..\/images\/178285_2963555.jpg","object_date_display":"1959","object_artist_culture_display":"Roy Lichtenstein<br \/>\nAmerican (1923-1997)","object_artist_sort_name":"Lichtenstein, Roy","object_tag_list":""},"artwork_205143":{"artwork_link":"..\/205143","object_title":"Brushstroke Abstraction I","object_date_begin":"1\/1\/1996","object_date_end":"12\/31\/1996","object_image_thumbnail":"..\/images\/182105_2982164.jpg","object_image_medium":"..\/images\/182105_2982163.jpg","object_date_display":"1996","object_artist_culture_display":"Roy Lichtenstein<br \/>\nAmerican (1923 - 1997)","object_artist_sort_name":"Lichtenstein, Roy","object_tag_list":""},"artwork_205145":{"artwork_link":"..\/205145","object_title":"Brushstroke Abstraction II","object_date_begin":"1\/1\/1996","object_date_end":"12\/31\/1996","object_image_thumbnail":"..\/images\/182106_2982180.jpg","object_image_medium":"