    <div id="page-title-wrapper">
              <h1 class="title">Landscape with the Ruins of the Castle of Egmond</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
