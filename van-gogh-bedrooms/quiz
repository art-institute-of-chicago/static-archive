<section id="quiz" ng-app="vanGoghQuiz">

  <div class="row">
    <div class="small-12 columns">
      <div id="vangogh-quiz" ng-controller="QuizController">
        <div class="quiz-container">
          <h1><span>Van Gogh</span> in Pop Culture</h1>

          <div class="quiz-progress">
            <div class="
              {{ ($index === activeQuestion) ? 'on' : 'off' }}
              {{ (question.questionState === 'answered') ? 'answered' : 'unanswered' }}
              {{ (question.correctness === 'correct') ? 'correct' : 'incorrect' }}
              " ng-repeat="question in questions"></div>
          </div> <!-- .quiz-progress -->

          <div class="quiz-outer-wrapper">
            <div class="quiz-inner-wrapper">

              <div class="intro {{ ( activeQuestion > -1 ) ? 'inactive' : 'active'  }}">
                <h2>How much do you know?</h2>
                <img class="portrait left" alt="Van Gogh Keyhole" src="keyHole-vangogh.png">
                <p>Though largely unknown in his lifetime, Vincent van Gogh has since become one of the beloved artists in the world. His life and work has been explored and referenced in popular culture across media. Test your knowledge of the Dutch painterâ€™s enduring legacy.</p>
                <a class="button large right" ng-click="activeQuestion = 0">Take the Quiz <i class="fa fa-arrow-right"></i></a>
              </div><!-- .intro -->

              <div class="question-container
                {{ ($index === activeQuestion) ? 'active' : 'inactive'  }}
                {{ (question.questionState === 'answered') ? 'answered' : 'unanswered'  }}
              "
                ng-repeat="question in questions"
              >
                <h2 class="question" ng-bind-html="question.question | to_trusted">{{ question.question }}</h2>
                <p class="help">{{ question.help }}</p>
                <p class="answer button"
                    ng-class="{
                      selected:isSelected($parent.$index, $index),
                      correct:isCorrect($parent.$index, $index),
                      image:answer.image,
                      tall:answer.tall
                    }"
                    ng-style="{ 'background-image': (answer.image) ? 'url(quiz/answers/{{answer.image}})' : 'none' }"
                    ng-click="selectAnswer($parent.$index, $index)"
                    ng-repeat="answer in question.answers" >
                    <span ng-bind-html="answer.text | to_trusted">{{ answer.text }}</span>
                </p>

                <div class="feedback">
                  <p ng-show="question.feedback" ng-bind-html="question.feedback | to_trusted">{{ question.feedback }}</p>
                  <p class="button small" ng-click="nextQuestion()">
                    <span ng-show="totalQuestions !== activeQuestion + 1">Next Question</span>
                    <span ng-show="totalQuestions === activeQuestion + 1">View Results</span>
                    <i class="fa fa-arrow-right"></i></p>
                </div>

              </div> <!-- .question-container -->

              <div class="results {{ (totalQuestions === activeQuestion) ? 'active' : 'inactive' }}">
                <h2 ng-bind-html="earnedResult.title | to_trusted">{{ earnedResult.title }}</h2>
                <img class="portrait left" alt="Van Gogh Bedroom"  ng-src="{{ earnedResult.image }}">
                <p>You correctly answered {{ score }} out of {{ totalQuestions }} questions.</p>
                <p ng-bind-html="earnedResult.description | to_trusted">{{ earnedResult.description }}</p>
                <div class="share-results">
                  <h3>Share your results</h3>

                  <a id="social-share-facebook" href="https://www.facebook.com/sharer/sharer.php?u=/van-gogh-bedrooms/quiz?score={{ percentage }}" class="button share-facebook small">Share on Facebook&nbsp;<i class="fa fa-facebook"></i></a>
                  <a id="social-share-twitter" href="http://twitter.com/share?url=/van-gogh-bedrooms/quiz?score={{percentage}}&text={{ earnedResult.title }} Take the Van Gogh in Pop Culture Quiz%20%40artinstitutechi" class="button share-twitter small">Share on Twitter&nbsp;<i class="fa fa-twitter"></i></a>

                </div> <!-- .share-results -->
                <p><small ng-bind-html="earnedResult.image_caption | to_trusted">{{ earnedResult.image_caption }}</small></p>
              </div><!-- .results -->
            </div><!-- .quiz-inner-wrapper -->
          </div><!-- .quiz-outer-wrapper -->
        </div><!-- .quiz-container -->
      </div><!-- #vangogh-quiz -->
    </div><!-- .small-12.columns -->
  </div><!-- .row -->

