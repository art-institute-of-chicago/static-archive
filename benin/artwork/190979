    <div id="page-title-wrapper">
              <h1 class="title">Oba Akenzua II Greeting Queen Elizabeth II</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
