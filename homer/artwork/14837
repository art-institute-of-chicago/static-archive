    <div id="page-title-wrapper">
              <h1 class="title">Netting the Fish</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
<script type="text/javascript">var re="";</script>
<div class="artwork-content">

        <div id="artwork-image">
            <a href="..\/images\/50632_4139652.jpg" class="enlargement-link enlargement-link-14837"><img src="../images/50632_4139654.jpg" alt="Netting the Fish" title="Winslow Homer
American, 1836-1910, Netting the Fish"  /></a>          </div>

    <p class="tools">
              <a href="..\/images\/50632_4139652.jpg" class="enlargement-link enlargement-link-14837"><strong>View</strong> enlargement</a><br />
                    <a href="../artwork/14837/zoomify" class="zoomify-link"><strong>Zoom</strong> image</a><br />
                    <a href="../artwork/14837/email"><strong>Email</strong> to a friend</a><br />
