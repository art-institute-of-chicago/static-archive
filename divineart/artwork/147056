    <div id="page-title-wrapper">
              <h1 class="title">The Battle of Actium from The Story of Caesar and Cleopatra</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
