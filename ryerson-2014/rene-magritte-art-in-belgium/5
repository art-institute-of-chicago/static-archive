      //Doing this because image attributes don't get properly read until everything is loaded.
      $(window).load(function() {
          $(".galleryImage img.scaled").each( function (index) {
              var img = $(this);
              var max = 112;
              // fix dimensions
              if ( img.height() > img.width() ){
                  img.css( { "height":max, "width":"auto"} );
              }
              else { img.css( { "height":"auto", "width":max} ); }
          });
      });
    })(jQuery);
    
