    <div id="page-title-wrapper">
              <h1 class="title">On the Bank of the Seine, Bennecourt</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
