    <div id="page-title-wrapper">
              <h1 class="title">Day of the God (Mahana no Atua)</h1>
        </div>

    
    </div>
   <!-- /navigation -->

                				        <div id="content">
                    <div id="content-content" class="clear-block"> 
<script type="text/javascript">var re="";</script>
<div class="artwork-content">

        <div id="artwork-image">
            <a href="..\/images\/217872_4143105.jpg" class="enlargement-link enlargement-link-27943"><img src="images/217872_4143107.jpg" alt="Day of the God (Mahana no Atua)" title="Paul Gauguin
