    <div id="gallery-image-map"></div>

<script type="text/javascript">

  $.ajax({
    url: "53-ne.json",
    dataType: "json",
    success: function(data) {
      // if mobile, make clicks fire hovers for tooltips
      var mobile = false;

      $('#gallery-image-map').flexiMap(data, function() {
        setupTooltips('#gallery-image-map');
      });

      if(mobile) mobileTapToHover();

      createAllModals(data);
      if(mobile) removeTooltipsFromOwned();
    },
    error: function(xhr, status, error) {
      console.error(status + ': ' + error.message);
    }
  });

  $(window).resize(function() {
    $('#gallery-image-map').flexiMap('resize');
  });

</script>

  <ol id="joyride-schedule">
    <li data-id="gallery-nav" data-button="next"><p>Use the map to explore other galleries.</p></li>
  </ol>

  <script type="text/javascript">
    $(window).load(function() {
      createLastJoyrideStopAndBegin('#gallery-image-map');
    });
  </script>


  

  <script type="text/javascript">
    $.ajax({
      url: "floor-plan.json",
      dataType: "json",
      success: function(data) {
        floor_plan_data = data;

        // load floor plan fleximap
        $('#floor-plan-map').flexiMap(floor_plan_data, function() {
          setupTooltips('#floor-plan-map');
        });

        // resize it when nav accessed
        $('.gallery-map-link').bind('touchstart mouseover', function() {
          $('#floor-plan-map').flexiMap('resize', '540px', '369px');
        });
      },
      error: function(xhr, status, error) {
        console.error(status + ': ' + error.message);
      }
    });

    $(document).foundationNavigation();
  </script>

