    $.ajax({
      url: "floor-plan.json",
      dataType: "json",
      success: function(data) {
        floor_plan_data = data;

        // load floor plan fleximap
        $('#floor-plan-map').flexiMap(floor_plan_data, function() {
          setupTooltips('#floor-plan-map');
        });

        // resize it when nav accessed
        $('.gallery-map-link').bind('touchstart mouseover', function() {
          $('#floor-plan-map').flexiMap('resize', '540px', '369px');
        });
      },
      error: function(xhr, status, error) {
        console.error(status + ': ' + error.message);
      }
    });

    $(document).foundationNavigation();
  </script>

